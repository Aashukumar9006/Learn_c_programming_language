#include<stdio.h>
#include<conio.h>
int main()
{
	int n,a,b,c;
	printf("Enter a 3 digit number");
	scanf("%d",&n);
	a=n%10;
	n=n/10;
	b=n%10;
	n=n/10;
	c=n;
	if (a>b&&a>c)
	printf("A is greater digit");
	else if (b>a&&b>c)
	printf("B is greater digit");
	else 
	printf("C is greater digit");
	getch();
}
