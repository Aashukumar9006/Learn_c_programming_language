#include<stdio.h>
#include<conio.h>
int main()
{
	int n,a,b,c,a1,b1,c1,sum,t;
	printf("Enter 3 digit no.");
	scanf("%d",&n);
	t=n;
	a=n%10;
	n=n/10;
	b=n%10;
	n=n/10;
	c=n;
	a1=a*a*a;
	b1=b*b*b;
	c1=c*c*c;
	
	sum=a1+b1+c1;
	if (t==sum)
	printf("The number is Armstrong");
	else 
	printf("The number is not Armstrong");
	getch();
}
