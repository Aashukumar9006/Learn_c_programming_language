#include<stdio.h>
#include<conio.h>
int main()
{
	int a,b,c,d;
	printf("Enter four no.");
	scanf("%d %d %d %d",&a,&b,&c,&d);
	if (a>b&&a>c&&a>d)
	printf("A is greater number");
	else if (b>a&&b>c&&b>d)
	printf("B is greater number");
	else if (c>a&&c>b&&c>d)
	printf("C is greater number");
	else
	printf("D is gteater number");
	getch();
}
